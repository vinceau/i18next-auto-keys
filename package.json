{
  "name": "i18next-auto-keys",
  "version": "1.1.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "i18next-auto-keys": "dist/cli/generatePot.js"
  },
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.d.ts",
    "dist/cli/generatePot.js"
  ],
  "description": "Automatic translation key generation for i18next",
  "keywords": [
    "i18next",
    "translation",
    "keys",
    "generator",
    "webpack",
    "loader",
    "transformer"
  ],
  "author": "Vincent Au",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:js && npm run build:types && npm run build:clean",
    "build:js": "webpack --config webpack.build.config.js",
    "build:types": "node scripts/build-types.js",
    "build:clean": "node scripts/clean-build-artifacts.js",
    "clean": "rimraf dist",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/gettext-parser": "^8.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29",
    "@types/webpack": "^5",
    "dts-bundle-generator": "^9.5.1",
    "esbuild-loader": "^4.4.0",
    "gettext-parser": "^8.0.0",
    "jest": "^29",
    "memfs": "^4",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "source-map": "^0.7.6",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "unionfs": "^4",
    "webpack": "^5",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "glob": "^11.0.0",
    "schema-utils": "^4.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
